<EmberWormhole @to='navbar-wormhole'>
  <PageActionsButtons @pageActions={{this.pageActions}} />
</EmberWormhole>

<Forms::ArticleForm
  @model={{@model}}
  @onSubmit={{action 'submit'}}
  @onCoverPhotoLoaded={{action 'coverPhotoLoaded'}}
  @canPin={{this.canPin}}
  @errorMessage={{this.errorMessage}}
/>