<EmberWormhole @to='navbar-wormhole'>
  <PageActionsButtons @pageActions={{pageActions}} />
</EmberWormhole>

<Forms::BookForm
  @model={{@model}}
  @showScanner={{this.showScanner}}
  @device={{this.device}}
  @devices={{this.devices}}
  @lookingUpIsbn={{this.lookingUpIsbn}}
  @lookupIsbnError={{this.lookupIsbnError}}
  @errorMessage={{this.errorMessage}}
  @onSubmit={{this.submit}}
  @onCancel={{this.cancel}}
  @onToggleScanner={{this.toggleScanner}}
  @onDevicesFound={{this.onDevicesFound}}
  @onScanSuccess={{this.onScanSuccess}}
  @lookupIsbn={{this.lookupIsbn}}
  @onCoverPhotoLoaded={{this.coverPhotoLoaded}}
  @setContent={{this.setContent}}
/>