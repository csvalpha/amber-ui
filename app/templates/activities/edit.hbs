<Forms::ActivityForm
  @model={{@model}}
  @controller={{this}}
  @onSubmit={{action 'submit'}}
  @activityCategoryOptions={{this.activityCategoryOptions}}
  @onCoverPhotoLoaded={{action 'coverPhotoLoaded'}}
  @groups={{this.groups}}
  @activityHasForm={{this.activityHasForm}}
  @combinedErrors={{this.combinedErrors}}
  @errorMessage={{this.errorMessage}}
/>