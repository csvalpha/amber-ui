<Forms::GroupForm
  @model={{@model}}
  @onSubmit={{action 'submit'}}
  @onCancel={{action 'cancel'}}
  @onFileLoaded={{action 'fileLoaded'}}
  @groupKindOptions={{this.groupKindOptions}}
  @errorMessage={{this.errorMessage}}
  @users={{this.users}}
  @onAddUser={{action 'addUser'}}
  @onRemoveMembership={{action 'removeMembership'}}
/>